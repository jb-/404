{
    "background":
    {
        "scripts": ["background.js"],
        "persistent": true
    },
    "manifest_version": 2,
    "name": "404",
    "description": "Forced not-found",
    "version": "0.0.1",
    "browser_action":
    {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "webRequest",
        "http://*/",
        "https://*/"
    ],
    "web_accessible_resources": [
        "filterList.json"
    ]


}